{
  "version": 2,
  "name": "development-website",
  "framework": null,
  "installCommand": "echo 'No dependencies to install for static site'",
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/tetris/?",
      "dest": "/tetris/index.html"
    },
    {
      "src": "/collaborative-canvas/?",
      "dest": "/collaborative-canvas/index.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
